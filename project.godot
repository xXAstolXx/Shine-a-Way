config_version=4

_global_script_classes=[ {
"base": "KinematicBody2D",
"class": "Player",
"language": "GDScript",
"path": "res://scripts/Player.gd"
} ]
_global_script_class_icons={
"Player": ""
}

[application]

config/name="Shine A Way"
config/description="As an electrician you just want to do your job: fix the light.  

But experiments hide within this secret laboratory and block your path. A drone will help you to shine a light and figure out a way  in this dark place."
run/main_scene="res://scenes/Main_Menu.tscn"
boot_splash/image="res://assets/ui/MainMenu/Key Art I just wanna Do my Job.png"
boot_splash/bg_color=Color( 0.145098, 0.145098, 0.14902, 1 )
config/icon="res://icon.png"

[audio]

default_bus_layout="res://other/bus_layout/IJustWannaDoMyJob_bus_layout.tres"

[autoload]

Checkpoint="*res://scripts/Checkpoint_Global.gd"
Global="*res://scripts/Global.gd"

[debug]

settings/gdscript/max_call_stack=2048
settings/visual_script/max_call_stack=2048

[display]

window/size/width=1920
window/size/height=1080
window/size/fullscreen=true
window/stretch/mode="2d"
window/stretch/aspect="expand"

[gui]

common/drop_mouse_on_gui_input_disabled=true

